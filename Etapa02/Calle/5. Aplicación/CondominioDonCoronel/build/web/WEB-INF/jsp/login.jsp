<body>
		<div class="container-fluid">
		<div class="row-fluid">
			<div class="row-fluid">
				<div class="login-box">
					<div class="icons">
						<a href="#"><i class="icon-home"></i></a>
						<a href="#"><i class="icon-cog"></i></a>
					</div>
					<h2>Condominio~ Don Coronel - Panel de Administración</h2>
					<form class="form-horizontal" action="index.htm" method="post">
						<fieldset>
							
							<div class="input-prepend" title="Usuario">
								<span class="add-on"><i class="icon-user"></i></span>
								<input class="input-large span10" name="username" id="username" type="text" placeholder="Ingrese usuario"/>
							</div>
							<div class="clearfix"></div>

							<div class="input-prepend" title="Contraseña">
								<span class="add-on"><i class="icon-lock"></i></span>
								<input class="input-large span10" name="password" id="password" type="password" placeholder="Ingrese contraseña"/>
							</div>
							<div class="clearfix"></div>
							
							<div class="button-login">	
								<button type="submit" class="btn btn-primary"><i class="icon-off icon-white"></i> Acceder</button>
							</div>
							<div class="clearfix"></div>
					</form>
					<hr>
					
				</div><!--/span-->
			</div><!--/row-->
			
				</div><!--/fluid-row-->
				
